<script language="JavaScript">
function getLocaleLabel(lbl)
{
	var mainlbl = "";
	var templbl = "";
	mainlbl = lbl;
	for(var i=0;i<SiteMapsLocales.length;i++)
	{
		 templbl = SiteMapsLocales[i][0];
		 if (mainlbl == templbl) {
		 		mainlbl = SiteMapsLocales[i][1];
		 		break;
		 }
		 
	}	
	
	return mainlbl;
	
}
</script>

<!-- Now only calling Get_English_Labels for all languages.  It will pull back all labels. -->
