<script language="JavaScript">
<!-- FUNCTION TO SET FOCUS ON SEARCH BOX ** START ** -->
//onload function
function focusSearch()
{
   try{
	  if (document.layers) { //Netscape 4
		     searchObj = eval('document.q');
	  }
	  else if (navigator.userAgent.indexOf("Opera") != -1) { //Opera
		     searchObj = eval('document.all.q');
	    }
	  else if (document.all && !document.getElementById) { //IE 4
		     searchObj = eval('document.all.q');
	  }
		  else if (document.getElementById) { //Netscape 6 & IE 5 & FireFox
		     searchObjs = document.getElementsByName('q');
		     searchObj = searchObjs[0];
	  }
	  else {
	  }
	searchObj.focus();

   }
   catch(err)
   {

   }
	
};

//setup onload function
if(typeof window.addEventListener != 'undefined')
{
	//.. gecko, safari, konqueror and standard
	window.addEventListener('load', focusSearch, false);
}
else if(typeof document.addEventListener != 'undefined')
{
	//.. opera 7
	document.addEventListener('load', focusSearch, false);
}
else if(typeof window.attachEvent != 'undefined')
{
	//.. win/ie
	window.attachEvent('onload', focusSearch);
}

//** remove this condition to degrade older browsers
else
{
	//.. mac/ie5 and anything else that gets this far
	
	//if there's an existing onload function
	if(typeof window.onload == 'function')
	{
		//store it
		var existing = onload;
		
		//add new onload handler
		window.onload = function()
		{
			//call existing onload function
			existing();
			
			//call generic onload function
			focusSearch();
		};
	}
	else
	{
		//setup onload function
		window.onload = focusSearch;
	}
}
 
<!-- FUNCTION TO SET FOCUS ON SEARCH BOX ** END ** -->
</script>




